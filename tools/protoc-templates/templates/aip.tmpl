                if err := s.post{{ $method.GoName }}(ctx, {{ $resourceGoName | untitle }}s); err != nil {
                return nil, err
                }
            {{ end }}
            return &pb.{{ $method.Output.GoIdent.GoName }}{
            {{ $resourceGoName }}s: {{ $resourceGoName | untitle}}s,
            NextPageToken: nextPageToken,
            }, nil
            }
        {{ end -}}

    {{ end }}
{{- end }}
